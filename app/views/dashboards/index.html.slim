section.action.footer
  .row.align-items-center.justify-content-center
    .col-md-4
      = button_tag('Kembali', class: 'button-action')
    .col-md-8
      = button_tag('Selanjutnya', class: 'button-action')

javascript:
  var moreButton = $('#feature-more');
  var lessButton = $('#feature-less');


  moreButton.click(function(){
    var featureList = $('#feature-list');
    var featureDetail = $('#feature-detail');
    console.log('click')
    featureList.addClass('hide');
    featureDetail.removeClass('hide');
  })

  lessButton.click(function(){
    var featureList = $('#feature-list');
    var featureDetail = $('#feature-detail');
    featureList.removeClass("hide");
    featureDetail.addClass('hide');
  })